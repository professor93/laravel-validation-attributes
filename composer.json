{
    "name": "uzbek/laravel-validation-attributes",
    "description": "Laravel validation attributes",
    "keywords": [
        "uzbek",
        "laravel",
        "laravel-validation-attributes"
    ],
    "homepage": "https://github.com/professor93/laravel-validation-attributes",
    "license": "MIT",
    "authors": [
        {
            "name": "Inoyatulloh",
            "email": "mronx@ya.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0|^10.0",
        "spatie/laravel-package-tools": "^1.14",
        "uzbek/laravel-attribute-scanner": "^0.1"
    },
    "require-dev": {
        "laravel/pint": "^1.7",
        "nunomaduro/collision": "^6.4",
        "orchestra/testbench": "^7.24",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.4",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Uzbek\\LaravelValidationAttributes\\": "src"
        },
        "files": ["src/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Uzbek\\LaravelValidationAttributes\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uzbek\\LaravelValidationAttributes\\LaravelValidationAttributesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
