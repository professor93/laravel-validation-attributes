{
    "name": "uzbek/laravel-validation-attributes",
    "description": "Laravel validation attributes",
    "keywords": [
        "uzbek",
        "laravel",
        "laravel-validation-attributes"
    ],
    "homepage": "https://github.com/professor93/laravel-validation-attributes",
    "license": "MIT",
    "authors": [
        {
            "name": "Inoyatulloh",
            "email": "mronx@ya.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-package-tools": "^1.12",
        "symfony/finder": "^6.1",
        "uzbek/laravel-attribute-scanner": "^0.1.0",
        "wyrihaximus/list-classes-in-directory": "^1.5"
    },
    "require-dev": {
        "laravel/pint": "^1.1",
        "nunomaduro/collision": "^6.2",
        "orchestra/testbench": "^7.6",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Uzbek\\LaravelValidationAttributes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uzbek\\LaravelValidationAttributes\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uzbek\\LaravelValidationAttributes\\LaravelValidationAttributesServiceProvider"
            ],
            "aliases": {
                "LaravelValidationAttributes": "ValidationAttributes"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
